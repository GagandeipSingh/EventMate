<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.SCHEDULE_EXACT_ALARM"/>
    <uses-permission android:name="android.permission.POST_NOTIFICATIONS"/>

    <application
        android:name=".scheduler.ChannelClass"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EventMate"
        tools:targetApi="31">
        <activity
            android:name=".activities.AlreadyRegAct"
            android:exported="false" />
        <activity
            android:name=".activities.DashboardAct"
            android:exported="false" />
        <activity
            android:name=".activities.AboutAct"
            android:exported="false" />
        <activity
            android:name=".activities.RegParticipants"
            android:exported="false" />
        <activity
            android:name=".activities.DetailsEnroll"
            android:exported="false" />
        <activity
            android:name=".activities.EnrollAct"
            android:exported="false" />
        <activity
            android:name=".activities.AccountId"
            android:exported="false" />
        <activity
            android:name=".activities.UserType"
            android:exported="false" />
        <activity
            android:name=".activities.EventAdd"
            android:exported="false" />
        <activity
            android:name=".activities.EventAct"
            android:exported="false" />
        <activity
            android:name=".activities.Splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".progress.BaseActivity"
            android:exported="false" />
        <activity
            android:name=".activities.PasswordAct"
            android:exported="false" />
        <activity
            android:name=".activities.RegisterAct"
            android:exported="false" />
        <activity
            android:name=".activities.LoginAct"
            android:exported="false" />
<receiver android:name=".scheduler.NotificationReceiver"/>
    </application>

</manifest>